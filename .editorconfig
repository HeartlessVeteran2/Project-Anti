root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
max_line_length = 120
trim_trailing_whitespace = true



[*.{java}]
# Java specific settings
indent_size = 4

[*.{xml,html,json,yml,yaml}]
indent_size = 2

[*.md]
trim_trailing_whitespace = false
